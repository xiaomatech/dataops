{
  "KYLIN": {
    "KYLIN_JOB": [
      {
        "name": "kylin_job_process",
        "label": "kylin job Process",
        "description": "kylin job Process",
        "interval": 1,
        "scope": "HOST",
        "source": {
          "type": "PORT",
          "default_port": 7070,
          "reporting": {
            "ok": {
              "text": "TCP OK - {0:.3f}s response on port {1}"
            },
            "warning": {
              "text": "TCP OK - {0:.3f}s response on port {1}",
              "value": 1.5
            },
            "critical": {
              "text": "Connection failed: {0} to {1}:{2}",
              "value": 5.0
            }
          }
        }
      }
    ],
    "KYLIN_QUERY": [
      {
        "name": "kylin_query_process",
        "label": "kylin query Process",
        "description": "kylin query Process",
        "interval": 1,
        "scope": "HOST",
        "source": {
          "type": "PORT",
          "default_port": 7070,
          "reporting": {
            "ok": {
              "text": "TCP OK - {0:.3f}s response on port {1}"
            },
            "warning": {
              "text": "TCP OK - {0:.3f}s response on port {1}",
              "value": 1.5
            },
            "critical": {
              "text": "Connection failed: {0} to {1}:{2}",
              "value": 5.0
            }
          }
        }
      }
    ]
  }
}